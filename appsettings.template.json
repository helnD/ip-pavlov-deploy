{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AppDatabase": "Host={HOST};Port={PORT};Database=IpPavlov;User Id={USER ID};Password={PASSWORD}"
  },
  "DatabaseInitialization": {
    "CategoriesFile": "Setup/Database/Categories.xml"
  },
  "Images": {
    "Root": "Images",
    "Categories": "Categories",
    "Products": "Products",
    "Partners": "Partners",
    "Certificates": "Certificates"
  },
  "AllowedHosts": "*"
}
