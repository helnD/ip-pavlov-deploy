{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AppDatabase": "Host={HOST};Port={PORT};Database=IpPavlov;User Id={USER ID};Password={PASSWORD}"
  },
  "DatabaseInitialization": {
    "CategoriesFile": "Setup/Database/Categories.xml",
    "CertificatesFile": "Setup/Database/Certificates.xml",
    "PartnersFile": "Setup/Database/Partners.xml"
  },
  "Resources": {
    "Files": {
      "Root": "Files",
      "PriceList": "price-list.xlsx"
    },
    "Images": {
      "Root": "Images",
      "Categories": "Categories",
      "Products": "Products",
      "Partners": "Partners",
      "Certificates": "Certificates"
    }
  },
  "FrontendOrigins": {
    "DevOrigin": "http://localhost:3000",
    "ReleaseOrigin": "http://localhost:3000"
  },
  "AllowedHosts": "*"
}
